{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext", "scriptHost", "ES2016.Array.Include"
    ],
    "module": "commonjs",
    "strict": true,
     "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}